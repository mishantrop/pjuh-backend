{
  "name": "yet-another-package-json-dependencies-updates-checker-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mikro-orm/core": "5.1.3",
    "@mikro-orm/migrations": "5.1.3",
    "@mikro-orm/nestjs": "5.0.1",
    "@mikro-orm/postgresql": "5.1.3",
    "@mikro-orm/sql-highlighter": "1.0.1",
    "@nestjs/axios": "0.0.7",
    "@nestjs/common": "8.4.4",
    "@nestjs/config": "2.0.0",
    "@nestjs/core": "8.4.4",
    "@nestjs/platform-express": "8.4.4",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "path": "0.12.7",
    "query-registry": "2.3.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "semver": "7.3.7"
  },
  "devDependencies": {
    "@mikro-orm/cli": "5.1.3",
    "@nestjs/cli": "8.2.5",
    "@nestjs/schematics": "8.0.10",
    "@nestjs/testing": "8.4.4",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/multer": "1.4.7",
    "@types/node": "17.0.30",
    "@types/semver": "7.3.9",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.0.0",
    "prettier": "2.6.2",
    "source-map-support": "0.5.21",
    "supertest": "6.2.3",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.9",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/src/mikro-orm.config.js"
    ]
  }
}
